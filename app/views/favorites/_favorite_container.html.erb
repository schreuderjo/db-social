<div class="resources-container" data-favorite-container-id=<%= resource.id%>>
    <ul>
    <% @resources.each do |resource| %>
      
        <li class="resource-link"><%= link_to resource.name, resource.url %> | <%=link_to 'Add to Favorite Resources', class: "add-to-favorites-link", remote:true %></li>
        <li class="resource-description"><%= resource.description%></li>
 
    <% end %>
    </ul>
  </div>